{
  "compilers": {
    "less": {
      "autoPrefix": "last 2 versions, > 5%",
      "cssComb": "none",
      "ieCompat": true,
      "strictMath": false,
      "strictUnits": false,
      "relativeUrls": true,
      "rootPath": "",
      "soruceMapRoot": null,
      "soruceMapBasePath": null,
      "sourceMap": true
    },
    "sass": {
      "includePath": null,
      "indentType": "space",
      "indentWidth": 2,
      "outputStyle": "nested",
      "Precision": 5,
      "relativeUrls": true,
      "soruceMapRoot": null,
      "sourceMap": false
    },
    "stylus": {
      "sourceMap": false
    },
    "babel": {
      "sourceMap": false
    },
    "coffeescript": {
      "bare": false,
      "runtimeMode": "node",
      "sourceMap": false
    }
  },
  "minifiers": {
    "css": {
      "enabled": false,
      "termSemicolons": true,
      "gzip": false
    },
    "javascript": {
      "enabled": false,
      "termSemicolons": true,
      "gzip": false
    }
  }
}